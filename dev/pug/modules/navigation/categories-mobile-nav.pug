nav.categories-mobile-nav
    ul.categories-mobile-nav__list
        - var list = 0;
        while list < 3
            - var column = 0;
            +liDivSpanTextDiv("categories-mobile", "-nav", "Category"+list, "", list++, "123")
                //.catalog-subnav(data-simplebar)
                .categories-mobile-subnav
                    .categories-mobile-subnav__header--link
                        a(href="").categories-mobile-subnav__title Subcategoriess
                            +countText(123, "")
                    - var block = 0;

                    while block < 2
                        +blockHedearTitle("categories-mobile", "-subnav", "123", "Sub"+column+list+block++, "")
                            ul.categories-mobile-subnav__list
                                - var n = 0;
                                while n < 2
                                    +listLinkSpanCount("categories-mobile-subnav", "123", "Product"+n++, "")







