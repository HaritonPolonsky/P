section.s-contacts
    .container
        +sectionHeader("Contacts")
        .contacts
            .contacts__container
                .contacts__body
                    .contacts__map
                        //<iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3582.8998998604625!2d-80.12437791434748!3d26.102186083483343!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x88d90069770272df%3A0x261cf7e091f79937!2zMTUzNSBTRSAxN3RoIFN0ICMxMDcsIEZvcnQgTGF1ZGVyZGFsZSwgRkwgMzMzMTYsINeQ16jXpteV16og15TXkdeo15nXqg!5e0!3m2!1siw!2sil!4v1552408667269" width="100%" height="100%" frameborder="0" style="border:0"></iframe>
                    +contacts("contacts", "", "Office", "1535 SE 17th Street, Suite 107 Fort Lauderdale, FL 33316", "Open hours", "Monday-Thursday 9:00-18:00", "Friday 9:00-12:00")
                    
                        +contactsListIcons("contacts__block", "icon")

                .contacts__body
                    .contacts__map
                        //<iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3582.8998998604625!2d-80.12437791434748!3d26.102186083483343!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x88d90069770272df%3A0x261cf7e091f79937!2zMTUzNSBTRSAxN3RoIFN0ICMxMDcsIEZvcnQgTGF1ZGVyZGFsZSwgRkwgMzMzMTYsINeQ16jXpteV16og15TXkdeo15nXqg!5e0!3m2!1siw!2sil!4v1552408667269" width="100%" height="100%" frameborder="0" style="border:0"></iframe>
                    +contacts("contacts", "", "Warehouse", "1535 SE 17th Street, Suite 107 Fort Lauderdale, FL 33316", "Open hours", "Monday-Thursday 9:00-18:00", "Friday 9:00-12:00")
            .contacts-form
                span.contacts__title Contact Us 
                .contacts-form__wrap
                    span.contacts-form__descr Submit a Customer Service Request
                    form.form
                        .form__body
                            +formLine("number")
                                    +inputWrap("Order", " form-group--width-m form-group--require", "", "", "")
                                        +inputF("Order", "number", "order", "")
                            +formLine("Text")
                                +inputWrap("Text", "", "", "", "")
                                    +textArea("Hello ", "text", "", "")
                            +formLine("mail")
                                    +inputWrap("Email", " form-group--width-m form-group--require", "", "", "")
                                        +inputF("Email", "email", "mail", "")
                            +formLine("phone")
                                +inputWrap("Telephone", " form-group--width-m form-group--require", "", "", "")
                                    +inputF("+791234567890", "tel", "phone", "")

                        +formFooter("Send", " form-group--width-m", "", "privat", "")
                        
            .contacts-requisites
                span.contacts__title Requisites
                +divSpanA("contacts-requisites__item", "contacts-requisites__text", "World Headquarters", "Solgar, Inc.", "500 Willow Tree Road", "Leonia, NJ 07605", "USA")
                +divSpanA("contacts-requisites__item", "contacts-requisites__text", "World Headquarters", "Solgar, Inc.", "500 Willow Tree Road", "Leonia, NJ 07605", "USA")
                +divSpanA("contacts-requisites__item", "contacts-requisites__text", "World Headquarters", "Solgar, Inc.", "500 Willow Tree Road", "Leonia, NJ 07605", "USA")
 
                
    
